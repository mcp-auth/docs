{
  "plugin": [
    "typedoc-plugin-zod",
    "typedoc-plugin-markdown",
    "typedoc-plugin-frontmatter",
    "./typedoc-plugin-custom.mjs"
  ],
  "tsconfig": "./sdk/js/packages/mcp-auth/tsconfig.build.json",
  "entryPoints": [
    "./sdk/js/packages/mcp-auth/src/index.ts",
  ],
  "name": "MCP Auth Node.js SDK reference",
  "out": "./docs/references/js",
  "disableSources": true,
  "hidePageHeader": true,
  "hideBreadcrumbs": true,
  "useCodeBlocks": true,
  "expandObjects": true,
  "expandParameters": true,
  "readme": "none",
  "publicPath": "/references/js/",
}
